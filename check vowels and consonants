#include <stdio.h>
#include <iostream>
using namespace std;

int main()
{
    char s[]="how are you";
    int i, cnt=0, con=0;
   
    for(i=0; s[i]!='\0';i++){
     if(s[i]=='a' || s[i]=='o'||s[i]=='e' || s[i]=='i'||s[i]=='u'){
         cnt++;
     }   
     else if((s[i]>='a' && s[i]<='z')||(s[i]>='A' && s[i]<='Z')){
         con++;
     }
    }
    cout<<"vowels:"<<cnt;
    cout<<"consonants:"<<con;
 

    return 0;
}
