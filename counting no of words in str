#include <stdio.h>
#include <iostream>
using namespace std;

int main()
{
    char s[]="how are  you";
    int i,word=0;
   
    for(i=0; s[i]!='\0';i++){
         // checking for whitespaces
         if(s[i]==' ' && s[i-1]!=' ') word++;
         //else continue; not needed
             
         
    }
    cout<<"words:"<<word+1;


    return 0;
}
